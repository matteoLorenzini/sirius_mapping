    skos:broader ex:Physical_Forces ;
    skos:narrower ex:Handling_for_Photo_Shoot ,
        ex:Transit_In_House ,
        ex:Shipment_Loading_Unloading ,
        ex:Gravitational_Loads ,
        ex:Construction_Vibrations ,
        ex:Excavation .

ex:Cumulative_Forces_High_Incidence_Low_Intensity a skos:Concept ;
    skos:inScheme ex:DamageCausingAgentsScheme ;
    skos:prefLabel "Cumulative forces (high incidence, low intensity)"@en ;
    skos:broader ex:Physical_Forces ;
    skos:narrower ex:Shipping_Cumulative ,
        ex:Gravity_Improper_Supports ,
        ex:Subsidence .

ex:Low_Level_Forces_Variable_Incidence_Low_Intensity a skos:Concept ;
    skos:inScheme ex:DamageCausingAgentsScheme ;
    skos:prefLabel "Low level forces (variable incidence, low intensity)"@en ;
